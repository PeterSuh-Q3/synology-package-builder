# DSM 6.3 ~ 6.99 에서는 Node.js v12 사용
[Node.js_v12]
dsm_min_ver = 6.3-00000
dsm_max_ver = 6.99-99999
priority = 10

# DSM 7.0 ~ 7.1.x 에서는 Node.js v16 사용
[Node.js_v16]
dsm_min_ver = 7.0-00000
dsm_max_ver = 7.1-99999
priority = 10

# DSM 7.2 이상에서는 Node.js v18 사용
[Node.js_v18]
os_min_ver = 7.2-00000
priority = 10
